c, r = input().split()
c = int(c)
r = int(r)

x = 0
y= 0
a= 999
b = 999
while (a !=0 or b!=0):
  a, b= input().split()
  a = int(a)
  b = int(b)
  x+=int(a)
  y+=int(b)
  if x > c:
    x = c
  elif x < 0:
    x = 0
  if y > r:
    y = r
  elif y < 0:
    y = 0
  if (a !=0 or b!=0):
    print(str(x) + " " + str(y))
